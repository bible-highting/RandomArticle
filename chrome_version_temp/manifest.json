{
  "manifest_version": 3,
  "name": "RandomArticle",
  "description": "This app recommends only one article from your backlog of saved content.",
  "version": "1.0",
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "action": {
    "default_icon": "images/logo32.png",
    "default_title": "RandomArticle",
    "default_popup": "popup/popup.html"
  }
}
